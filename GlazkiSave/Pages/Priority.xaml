<Page x:Class="GlazkiSave.Pages.Priority"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GlazkiSave.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Priority">

    <Grid Background="White">
        <TextBlock HorizontalAlignment="Left" Margin="325,10,0,0" FontSize="18" Text="Приоритет агентов" VerticalAlignment="Top"/>
        <Button Content="Назад" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" Width="75" Click="BackClick"/>
        <Button Content="Сохранить" HorizontalAlignment="Right" Margin="0,0,25,10" VerticalAlignment="Bottom" Width="90"/>
    </Grid>
</Page>
